{
  "name": "clean-mark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3020",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint",
    "lint:fix": "biome lint --apply",
    "format": "biome format --write",
    "format:src": "biome format --write src/",
    "format:check": "biome format",
    "check": "biome check",
